{
  "name": "@inertiajs/vue3",
  "version": "2.1.1",
  "license": "MIT",
  "description": "The Vue 3 adapter for Inertia.js",
  "contributors": [
    "Jonathan Reinink <jonathan@reinink.ca>"
  ],
  "homepage": "https://inertiajs.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/inertiajs/inertia.git",
    "directory": "packages/vue3"
  },
  "bugs": {
    "url": "https://github.com/inertiajs/inertia/issues"
  },
  "files": [
    "dist",
    "types"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./server": {
      "types": "./types/server.d.ts",
      "import": "./dist/server.esm.js",
      "require": "./dist/server.js"
    }
  },
  "typesVersions": {
    "*": {
      "server": [
        "types/server.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "./build.js --watch",
    "build": "pnpm clean && ./build.js && tsc --emitDeclarationOnly --skipLibCheck",
    "clean": "rm -rf types && rm -rf dist",
    "prepublishOnly": "pnpm build",
    "canary:build": "./build.js --canary",
    "canary:check": "es-check es2020 \"dist/canary.*.js\" --checkFeatures --module",
    "canary": "pnpm canary:build && pnpm canary:check"
  },
  "devDependencies": {
    "@types/node": "^22.5.3",
    "axios": "^1.8.2",
    "es-check": "^9.3.1",
    "esbuild": "^0.25.0",
    "esbuild-node-externals": "^1.6.0",
    "typescript": "^4.9.4",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@inertiajs/core": "workspace:*",
    "es-toolkit": "^1.33.0"
  }
}
