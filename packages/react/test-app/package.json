{
  "type": "module",
  "scripts": {
    "build": "vite build .",
    "build:ssr": "vite build --ssr ssr.tsx --outDir dist",
    "build:ssr-auto": "vite build --config vite.config.ssr-auto.ts --ssr ssr-auto.tsx --outDir dist",
    "dev": "nodemon --watch . --watch ../../core/dist --watch ../../react/dist --ext js,tsx,html,json --ignore dist/ --exec 'vite build .'",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "nodemon": "^3.1.11",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@inertiajs/core": "workspace:*",
    "@inertiajs/react": "workspace:*",
    "@inertiajs/vite": "workspace:*",
    "@vitejs/plugin-react": "^5.1.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  }
}
