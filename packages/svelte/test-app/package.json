    "svelte": "^4.2.20",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.20"
  },
  "dependencies": {
    "@inertiajs/core": "workspace:*",
    "@inertiajs/svelte": "workspace:*"
  }
