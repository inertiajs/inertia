{
  "type": "module",
  "scripts": {
    "build": "vite build .",
    "dev": "nodemon --watch . --watch ../../core/dist --watch ../../svelte/dist --ext js,ts,svelte,html,json --ignore dist/ --exec 'vite build .'",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tsconfig/svelte": "^5.0.4",
    "svelte": "^4.2.14",
    "typescript": "^5.6.2",
    "vite": "^5.4.19",
    "nodemon": "^3.0.0"
  },
  "dependencies": {
    "@inertiajs/core": "workspace:*",
    "@inertiajs/svelte": "workspace:*"
  }
}